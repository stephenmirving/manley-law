{
  "name": "manley-law",
  "version": "1.0.1",
  "description": "The business website for the Manley Law Firm in Moncks Corner, SC",
  "main": "index.html",
  "scripts": {
    "dev": "sass --watch --update --style=expanded www/sass/style.scss:www/css/style-v106.css",
    "prod": "sass --charset --no-source-map --style=compressed www/sass/style.scss:www/css/style-v106.css && cleancss --inline 'local,remote,!fonts.googleapis.com' -o www/css/style-v106.css www/css/style-v106.css && terser www/js/main.js --compress ecma=2021,unsafe_comps=true,unsafe_arrows=true,unsafe_undefined=true,passes=3 --mangle --output www/js/main-v102.min.js",
    "payment-style": "sass --watch --update --no-charset --no-source-map --style=compressed www/sass/cardpointe/sass/cardpointe.scss:www/sass/cardpointe/css/cardpointe.css",
    "payment-style-remin": "cleancss -o www/sass/cardpointe/css/cardpointe.min.css www/sass/cardpointe/css/cardpointe.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephenmirving/manley-law.git"
  },
  "keywords": [
    "Manley",
    "Halverstadt",
    "Law",
    "Firm",
    "Moncks",
    "Corner",
    "Charleston",
    "Business",
    "Website"
  ],
  "author": "Stephen M Irving",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stephenmirving/manley-law/issues"
  },
  "homepage": "https://github.com/stephenmirving/manley-law#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "htmlhint": "^1.1.4",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "postcss-scss": "^4.0.9",
    "postcss-sorting": "^8.0.2",
    "prettier": "^3.2.4",
    "purgecss": "^5.0.0",
    "stylelint": "^16.2.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-group-selectors": "^1.0.10",
    "stylelint-high-performance-animation": "^1.10.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0",
    "stylelint-rem-over-px": "^1.0.1",
    "stylelint-scss": "^6.1.0"
  }
}
